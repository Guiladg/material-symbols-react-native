{
  "name": "material-symbols-react-native",
  "version": "1.0.1",
  "description": "An icon placer component for Google's Material Symbols to use with React Native.",
  "types": "index.d.ts",
  "files": [
    "*.js",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Guiladg/material-symbols-react-native.git"
  },
  "keywords": [
    "material design",
    "symbols",
    "icons",
    "android"
  ],
  "author": "guiladg <guiladg@gmail.com> (https://github.com/Guiladg)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Guiladg/material-symbols-react-native/issues"
  },
  "homepage": "https://github.com/Guiladg/material-symbols-react-native#readme",
  "peerDependencies": {
    "react": ">= 16.7.0",
    "react-native": ">= 0.70.0",
    "react-native-svg": ">= 13.6.0"
  },
  "devDependencies": {
    "@material-design-icons/scripts": "0.5.2",
    "@types/react": ">= 16.7.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.0.3",
    "progress": "^2.0.3",
    "typescript": "^5.2.2"
  },
  "workspaces": [
    "definitions/outlined/300",
    "definitions/rounded/300",
    "definitions/sharp/300",
    "definitions/outlined/500",
    "definitions/rounded/500",
    "definitions/sharp/500",
    "definitions/outlined/600",
    "definitions/rounded/600",
    "definitions/sharp/600"
  ],
  "scripts": {
    "test": "echo \"No test specified\"",
    "download:definitions": "npm run download:100 && npm run download:200  && npm run download:300 && npm run download:400 && npm run download:500 && npm run download:600 && npm run download:700",
    "download:100": "npx @material-design-icons/scripts download svg --symbols --weight 100 --to downloads/100",
    "download:200": "npx @material-design-icons/scripts download svg --symbols --weight 200 --to downloads/200",
    "download:300": "npx @material-design-icons/scripts download svg --symbols --weight 300 --to downloads/300",
    "download:400": "npx @material-design-icons/scripts download svg --symbols --weight 400 --to downloads/400",
    "download:500": "npx @material-design-icons/scripts download svg --symbols --weight 500 --to downloads/500",
    "download:600": "npx @material-design-icons/scripts download svg --symbols --weight 600 --to downloads/600",
    "download:700": "npx @material-design-icons/scripts download svg --symbols --weight 700 --to downloads/700",
    "generate:definitions": "node ./scripts/generateDefs.mjs",
    "publish:all": "npm run publish:definitions && npm run publish:component",
    "publish:definitions": "npm publish -ws",
    "publish:component": "npm publish"
  }
}